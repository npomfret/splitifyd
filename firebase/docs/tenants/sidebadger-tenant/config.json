          "heading": "3xl",
          "display": "5xl"
        },
        "fluidScale": {
          "xs": "clamp(0.75rem, 0.7rem + 0.25vw, 0.875rem)",
          "sm": "clamp(0.875rem, 0.8rem + 0.375vw, 1rem)",
          "base": "clamp(1rem, 0.9rem + 0.5vw, 1.125rem)",
