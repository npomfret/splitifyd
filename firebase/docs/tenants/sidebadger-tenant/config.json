      "typography": {
        "fontFamily": {
          "sans": "Inter, system-ui, -apple-system, sans-serif",
          "serif": "Georgia, serif",
          "mono": "JetBrains Mono, monospace"
        },
        "sizes": {
          "xs": "0.75rem",
          "sm": "0.875rem",
          "md": "1rem",
          "lg": "1.125rem",
