        "easing": {
          "standard": "cubic-bezier(0.22, 1, 0.36, 1)",
          "decelerate": "cubic-bezier(0.05, 0.7, 0.1, 1)",
          "accelerate": "cubic-bezier(0.3, 0, 0.8, 0.15)",
          "spring": "cubic-bezier(0.34, 1.56, 0.64, 1)"
