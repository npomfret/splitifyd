            "accent": "#16A34A",
            "disabled": "#9CA3AF",
            "hero": "#14532D",
            "eyebrow": "#16A34A",
            "code": "#15803D",
            "owed": "#DC2626"
          },
          "interactive": {
            "primary": "#16A34A",
            "primaryHover": "#15803D",
            "primaryActive": "#166534",
            "primaryForeground": "#FFFFFF",
            "secondary": "#DCFCE7",
            "secondaryHover": "#BBF7D0",
            "secondaryActive": "#86EFAC",
            "secondaryForeground": "#14532D",
            "destructive": "#DC2626",
            "destructiveHover": "#B91C1C",
            "destructiveActive": "#991B1B",
            "destructiveForeground": "#FFFFFF",
            "accent": "#16A34A",
            "ghost": "#4ADE80",
            "magnetic": "#22C55E",
            "glow": "#4ADE80"
          },
          "border": {
            "subtle": "#86EFAC",
            "default": "#4ADE80",
            "strong": "#22C55E",
            "focus": "#16A34A",
            "warning": "#D97706",
            "error": "#DC2626"
          },
          "status": {
            "success": "#16A34A",
            "warning": "#D97706",
            "danger": "#DC2626",
            "info": "#0284C7"
          },
          "gradient": {
            "primary": ["#16A34A", "#15803D"],
            "accent": ["#22C55E", "#16A34A"],
            "aurora": ["#16A34A", "#22C55E", "#4ADE80", "#86EFAC"],
            "text": ["#14532D", "#166534"]
