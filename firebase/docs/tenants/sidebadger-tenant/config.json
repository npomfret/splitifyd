{
  "id": "sidebadger-tenant",
  "domains": [
    "demo.sidebadger.com",
    "demo.sidebadger.me"
  ],
  "branding": {
    "primaryColor": "#16A34A",
    "secondaryColor": "#22C55E",
    "accentColor": "#15803D",
    "showAppNameInHeader": false
  },
  "marketingFlags": {
    "showMarketingContent": true,
    "showPricingPage": true
  },
  "brandingTokens": {
    "tokens": {
      "version": 1,
      "palette": {
        "primary": "#16A34A",
        "primaryVariant": "#15803D",
        "secondary": "#22C55E",
        "secondaryVariant": "#4ADE80",
        "accent": "#15803D",
        "neutral": "#FFFFFF",
        "neutralVariant": "#F0FDF4",
        "success": "#16A34A",
        "warning": "#D97706",
        "danger": "#DC2626",
        "info": "#0284C7"
