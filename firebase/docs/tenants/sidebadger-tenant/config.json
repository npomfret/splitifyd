      },
      "spacing": {
        "2xs": "0.125rem",
        "xs": "0.25rem",
        "sm": "0.5rem",
        "md": "0.75rem",
        "lg": "1rem",
        "xl": "1.5rem",
        "2xl": "2rem"
      },
      "radii": {
        "none": "0px",
        "sm": "6px",
        "md": "10px",
        "lg": "16px",
        "pill": "999px",
