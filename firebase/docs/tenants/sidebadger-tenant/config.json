            "secondaryForeground": "#0a2818",
            "destructive": "#EF4444",
            "destructiveHover": "#F87171",
            "destructiveActive": "#DC2626",
            "destructiveForeground": "#FFFFFF",
            "accent": "#f0c05c",
            "ghost": "#1a3f2a",
            "magnetic": "#9dd4a3",
            "glow": "#7ec786"
          },
          "border": {
            "subtle": "rgba(240, 192, 92, 0.15)",
